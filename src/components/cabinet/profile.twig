    <div class="profile">
        <div class="profile-data">
            <h2 class="profile-title">Данные профиля</h2>
            <form action="#" method="post" class="profile-data__form">
                <div class="profile-data__grid">
                    <div class="profile-data__item">
                        <input type="text" name="profile-name" class="c-input" placeholder="Ваше имя и фамилия:">
                    </div>
                    <div class="profile-data__item">
                        <input type="text" name="profile-email" class="c-input" placeholder="Email">
                    </div>
                    <div class="profile-data__item">
                        <input type="text" name="profile-phone" class="c-input" placeholder="Телефон">
                    </div>
                    <div class="profile-data__item">
                        <input type="text" name="profile-address" class="c-input" placeholder="Где проживаете">
                    </div>
                </div>
                <button class="c-button c-button__medium">Сохранить</button>
            </form>
        </div>
        <div class="profile-address">
            <h2 class="profile-title">Адрес доставки</h2>
            <div class="profile-address__card">
                <div class="profile-address__grid">
                    <div class="profile-address__item">
                        <input type="text" name="profile-city" class="c-input" placeholder="Введите город:">
                    </div>
                    <div class="profile-address__item">
                        <input type="text" name="profile-street" class="c-input" placeholder="Улица">
                    </div>
                    <div class="profile-address__item">
                        <input type="text" name="profile-house" class="c-input" placeholder="Дом">
                    </div>
                    <div class="profile-address__item">
                        <input type="text" name="profile-build" class="c-input" placeholder="Строение">
                    </div>
                    <div class="profile-address__item">
                        <input type="text" name="profile-body" class="c-input" placeholder="Корпус">
                    </div>
                    <div class="profile-address__item">
                        <input type="text" name="profile-room" class="c-input" placeholder="Квартира">
                    </div>
                </div>
                <div class="profile-address-operation">
                    <a href="#" class="profile-address-operation__btn add"></a>
                </div>
            </div>

            <div class="profile-address__clone">
                <div class="profile-address__card">
                    <div class="profile-address__grid">
                        <div class="profile-address__item">
                            <input type="text" name="profile-city" class="c-input" placeholder="Введите город:">
                        </div>
                        <div class="profile-address__item">
                            <input type="text" name="profile-street" class="c-input" placeholder="Улица">
                        </div>
                        <div class="profile-address__item">
                            <input type="text" name="profile-house" class="c-input" placeholder="Дом">
                        </div>
                        <div class="profile-address__item">
                            <input type="text" name="profile-build" class="c-input" placeholder="Строение">
                        </div>
                        <div class="profile-address__item">
                            <input type="text" name="profile-body" class="c-input" placeholder="Корпус">
                        </div>
                        <div class="profile-address__item">
                            <input type="text" name="profile-room" class="c-input" placeholder="Квартира">
                        </div>
                    </div>
                    <div class="profile-address-operation">
                        <a href="#" class="profile-address-operation__btn delete"></a>
                    </div>
                </div>
            </div>

            <div class="profile-address__some"></div>
            <div class="profile-address__footer">
                <button class="c-button c-button__medium">Сохранить</button>
            </div>



